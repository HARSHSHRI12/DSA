#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int>vname;
   
    for(int i=0;i<5;i++){
        cout<<"Enter element in vector :";
        int element;
        cin>>element;
        vname.push_back(element);
        
    }
    int x;
    int occurrence=-1;
    cout<<"Enter x: ";
    cin>>x;
    for(int i=0;i<vname.size();i++){
        if(vname[i]==x){
            occurrence=i;
        }
    }
    cout<<"the last occurrence is :"<<occurrence<<endl;
    return 0;
}
