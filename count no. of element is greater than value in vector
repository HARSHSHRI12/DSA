#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int>vname;
    for(int i=0;i<5;i++){
        int element;
        cout<<"Enter element :";
        cin>>element;
        vname.push_back(element);
    }
    int value;
    int count=0;
    cout<<"Enter value :";
    cin>>value;
    for(int i=0;i<vname.size();i++){
        if(vname[i]>value){
            count++;
        }
    }
    cout<<"the total count of occurrence which is the greater than value:"<< value<<" "<<"is : "<<count;
    return 0;
}
